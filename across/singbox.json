{
  "log": {
    "disabled": false,
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "address": "8.8.8.8",
        "detour": "proxy_out"
      },
      {
        "tag": "dns_direct",
        "address": "114.114.114.114",
        "detour": "direct_out"
      }
    ],
    "rules": [
      {
        "rule_set": "china_site",
        "server": "dns_direct"
      }
    ],
    "strategy": "ipv4_only",
    "independent_cache": true,
    "cache_capacity": 0,
    "reverse_mapping": false
  },
  "inbounds": [
    {
      "tag": "tun_main",
      "type": "tun",
      "address": [
        "10.10.10.0/30"
      ],
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "stack": "system"
    }
  ],
  "outbounds": [
    {
      "tag": "direct_out",
      "type": "direct"
    },
    {
      "type": "selector",
      "tag": "proxy_out",
      "outbounds": [
        "cloudflare",
        "reality"
      ],
      "default": "reality",
      "interrupt_exist_connections": true
    },
    {
      "type": "vless",
      "tag": "cloudflare",
      "server": "104.19.0.0",
      "server_port": 8443,
      "uuid": "b38d3591-91c2-4e31-a97c-c444b9339b4e",
      "tls": {
        "enabled": true,
        "server_name": "chat.nireus.cc",
        "insecure": false
      },
      "transport": {
        "type": "ws",
        "path": "/deepseek",
        "headers": {
          "host": "chat.nireus.cc"
        }
      }
    },
    {
      "type": "vless",
      "tag": "reality",
      "server": "23.94.243.117",
      "server_port": 60777,
      "uuid": "0bbf8374-12a6-406f-fdf6-17fabeb463a7",
      "flow": "xtls-rprx-vision",
      "network": "tcp",
      "tls": {
        "server_name": "chat.nireus.cc",
        "alpn": [
          "h2"
        ],
        "utls": {
          "enabled": true,
          "fingerprint": "safari"
        },
        "reality": {
          "enabled": true,
          "public_key": "fPHwscp1bpYvJPJyr_8S0rNefqskOWtEnCi-hiIrmD0",
          "short_id": "f5f649a2"
        },
        "enabled": true
      },
      "packet_encoding": "xudp"
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": "tun_main",
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "direct_out"
      },
      {
        "rule_set": "china_site",
        "outbound": "direct_out"
      },
      {
        "rule_set": "china_ip_cidr",
        "outbound": "direct_out"
      }
    ],
    "rule_set": [
      {
        "tag": "china_ip_cidr",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
        "download_detour": "proxy_out",
        "update_interval": "1d"
      },
      {
        "tag": "china_site",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-cn.srs",
        "download_detour": "proxy_out",
        "update_interval": "1d"
      }
    ],
    "final": "proxy_out",
    "auto_detect_interface": true
  }
}
